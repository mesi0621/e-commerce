{
    "version": 2,
    "name": "modo-ecommerce-backend",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "MONGODB_URI": "@mongodb_uri",
        "JWT_SECRET": "@jwt_secret",
        "EMAIL_USER": "@email_user",
        "EMAIL_PASSWORD": "@email_password",
        "FRONTEND_URL": "@frontend_url"
    }
}